{
    "name": "vitsw/content-generator",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "illuminate/mail": "5.0.*",
        "irazasyed/larasupport": "~1.0",
        "roumen/sitemap": "^2.5",
        "doctrine/dbal": "^2.5",
        "phpoffice/phpexcel": "^1.8",
        "ezyang/htmlpurifier": "^4.7",
        "laravel/envoy": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeception/codeception": "~2.1@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
